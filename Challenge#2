const calcTip = (bill) => {
    if (bill >= 50 && bill <= 300){
        return bill * .15;
    } else {
        return bill * .20;
    }
}
 
let tip = calcTip(100);
console.log(tip);
 
const bills = [125, 555, 44];
const tips = [calcTip(bills[0]), calcTip(bills[1]), calcTip(bills[2])];
 
console.log(tips);
